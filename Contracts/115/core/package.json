{
  "name": "parallel",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=12.x"
  },
  "scripts": {
    "compile": "truffle compile && yarn generate-types && tsc",
    "generate-types": "typechain --target=truffle-v5 'build/contracts/*.json'",
    "postinstall": "truffle compile && yarn generate-types",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf build/ && rm -rf types/",
    "lint": "yarn lint:sol && yarn lint:js",
    "lint:sol": "./node_modules/solhint/solhint.js contracts/**/*.sol",
    "lint:js": "xo",
    "coverage": "yarn compile && truffle run coverage --file=\"./build/test/**/*.js\"",
    "test": "yarn clean && yarn hardhat clean && yarn hardhat compile && yarn compile && npx hardhat test",
    "test:file": "yarn hardhat clean && yarn hardhat compile && yarn clean && yarn compile && npx hardhat test",
    "retest:file": "yarn tsc && truffle test",
    "test:liquidityMining": "yarn clean && yarn compile && truffle test ./build/test/liquidityMining/**",
    "test:governance": "yarn clean && yarn compile && truffle test ./build/test/governance/**",
    "retest:governance": "yarn tsc && truffle test ./build/test/governance/**",
    "xo-fix": "xo --fix",
    "prettier": "./node_modules/.bin/prettier --write ./test/** ./contracts ./deploy ./scripts ./merkle ./utils hardhat.config.ts ./airdrop ./claims ./config",
    "prettier:solidity": "./node_modules/.bin/prettier --write contracts/**/*.sol",
    "generate-merkle-root": "ts-node merkle/generate-merkle-root.ts merkle/input.json",
    "verify-merkle-root": "ts-node merkle/verify-merkle-root.ts merkle/output.json",
    "verify-deployment": "npx hardhat test scripts/deployment/VerifyFullDeployment.test.ts",
    "verify-core": "npx hardhat test scripts/deployment/VerifyCore.test.ts",
    "verify-governance": "npx hardhat test scripts/deployment/VerifyGovernance.test.ts",
    "verify-grant-multisig": "npx hardhat test scripts/deployment/VerifyMultisigGrant.test.ts",
    "verify-contract": "yarn hardhat verify-single --config ./hardhat.verify.config.ts",
    "deploy-all": "npx hardhat run scripts/deployAll.ts",
    "deploy-core": "npx hardhat run scripts/deployCore.ts",
    "deploy-governance": "npx hardhat run scripts/deployGovernance.ts",
    "grant-multisig": "npx hardhat run scripts/grantMultisig.ts",
    "revoke-deployer": "npx hardhat run scripts/revokeDeployer.ts",
    "echidna-test": "./echidna-test/echidna-test --config echidna-config.yaml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.10",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-truffle5": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/hardhat-upgrades": "^1.10.0",
    "@openzeppelin/test-helpers": "^0.5.15",
    "@typechain/ethers-v5": "8.0.2",
    "@typechain/hardhat": "3.0.0",
    "@typechain/truffle-v5": "6.0.0",
    "@types/bn.js": "^4.11.6",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/underscore": "^1.10.22",
    "@types/web3": "^1.2.2",
    "abi-decoder": "^2.3.0",
    "eslint-config-xo-typescript": "^0.37.0",
    "eslint-plugin-prettier": "^3.3.1",
    "hardhat-deploy": "^0.8.11",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "mocha": "^9.2.0",
    "moment": "^2.29.1",
    "prettier": "^2.1.1",
    "prettier-plugin-solidity": "1.0.0-alpha.59",
    "solhint": "^3.2.0",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.20",
    "truffle": "^5.4.18",
    "truffle-contract-size": "^2.0.0",
    "ts-node": "^10.4.0",
    "typechain": "6.0.2",
    "typescript": "^4.4.4",
    "underscore": "^1.10.2",
    "xo": "^0.37.1"
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@atixlabs/hardhat-time-n-mine": "^0.0.5",
    "@ethersproject/abstract-provider": "^5.5.1",
    "@ethersproject/bignumber": "^5.5.0",
    "@ethersproject/contracts": "^5.5.0",
    "@openzeppelin/contracts": "^3.1.0",
    "@truffle/hdwallet-provider": "1.4.1",
    "@types/axios": "^0.14.0",
    "@types/chai-as-promised": "^7.1.5",
    "@types/fs-extra": "^9.0.13",
    "@types/ms": "^0.7.31",
    "@types/node": "^16.11.6",
    "axios": "^0.23.0",
    "chai": "^4.3.6",
    "cross-fetch": "^3.0.6",
    "csv2json": "^2.0.2",
    "csvtojson": "^2.0.10",
    "dotenv": "^8.2.0",
    "ethereum-waffle": "^3.4.4",
    "ethereumjs-util": "^7.0.9",
    "ethers": "^5.4.7",
    "fs-extra": "^10.1.0",
    "graphql": "^15.5.0",
    "hardhat": "^2.9.1",
    "hardhat-gas-reporter": "^1.0.8",
    "json2csv": "^5.0.6",
    "ms": "^2.1.3",
    "truffle-plugin-verify": "^0.5.25",
    "web3": "^1.2.11",
    "web3-eth-contract": "^1.2.11",
    "yargs": "^16.2.0"
  }
}
